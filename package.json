{
  "name": "remote-rsync",
  "displayName": "Remote-Rsync",
  "description": "It works similar to vscode cli",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "publisher": "Kushal Verma",
  "activationEvents": [
    "onCommand: remote-rsync.Remote-Rsync",
    "onCommand: AIX_Terminals"
  ],
  "contributes": {
    "commands": [
      {
        "command": "remote-rsync.Remote-Rsync",
        "title": "Remote-Rsync"
      },
      {
        "command": "AIX_Terminals",
        "title": "Open AIX terminals"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "fs-extra": "^11.3.1",
    "ssh2-sftp-client": "^12.0.1"
  }
}
